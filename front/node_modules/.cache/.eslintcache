erDescription(e.target.value)}\r\n        />\r\n      </div>\r\n      <button className=\" bg-blue-500 text-white py-2 px-4 rounded\" onClick={handleCreateOrder}>\r\n        Criar Ordem de Serviço\r\n      </button>\r\n\r\n      <Modal\r\n        isOpen={isModalOpen || errorModalOpen}\r\n        onRequestClose={closeModal}\r\n        contentLabel=\"Tag Saved\"\r\n        className=\"bg-white p-6 rounded-lg shadow-lg mx-auto my-32 max-w-screen-md\"\r\n        overlayClassName=\"overlay fixed inset-0 bg-black bg-opacity-50 flex justify-center items-center\"\r\n      >\r\n        <p className='font-bold'>{savedMessage}</p>\r\n        <button onClick={closeModal} className=\"mt-10 bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded flex justify-center items-center\">\r\n          Fechar\r\n        </button>\r\n      </Modal>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CreateOrder;\r\n","C:\\Users\\hugoj\\OneDrive\\Documentos\\Tag-RFID\\front\\src\\pages\\AssociarOS.jsx",[],[],"C:\\Users\\hugoj\\OneDrive\\Documentos\\Tag-RFID\\front\\src\\components\\JoinOS.jsx",[],[],"C:\\Users\\hugoj\\OneDrive\\Documentos\\Tag-RFID\\front\\src\\pages\\VisualizarOS.jsx",[],[],"C:\\Users\\hugoj\\OneDrive\\Documentos\\Tag-RFID\\front\\src\\components\\OsView.jsx",[],[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":9,"column":10,"nodeType":"120","messageId":"121","endLine":9,"endColumn":17},"dot-location",[],"new-parens",[],"no-mixed-operators",[],"no-new-object",["122"],"no-whitespace-before-property",[],"rest-spread-spacing",[],"no-unused-vars","'orderId' is assigned a value but never used.","Identifier","unusedVar","no-object-constructor"]